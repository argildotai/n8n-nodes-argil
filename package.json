{
    "name": "n8n-nodes-argil",
    "version": "0.1.5",
    "description": "n8n node for creating engaging videos with Argil AI",
    "keywords": [
        "n8n-community-node-package",
        "argil",
        "video",
        "ai",
        "video-generation"
    ],
    "license": "MIT",
    "homepage": "https://github.com/your-username/n8n-nodes-argil",
    "author": {
        "name": "Argil Community",
        "email": "support@argil.ai"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/your-username/n8n-nodes-argil.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && cp package.json dist/ && cp -r icons dist/ && cp icons/argil.svg dist/ && cp icons/argil.svg dist/nodes/Argil/",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials --ext .ts",
        "lintfix": "eslint nodes credentials --ext .ts --fix",
        "prepublishOnly": "npm run build && npm run lint"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/ArgilApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Argil/Argil.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^20.8.9",
        "@typescript-eslint/parser": "^6.8.0",
        "eslint": "^8.51.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.1",
        "n8n-workflow": "^1.54.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    },
    "peerDependencies": {
        "n8n-workflow": "^1.54.0"
    }
}
